<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="200.0" prefWidth="308.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.controller.ControllerUpdate">
    <children>
        <ImageView fitHeight="45.0" fitWidth="45.0" layoutX="128.0" layoutY="11.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@ic_logo.png" />
            </image>
        </ImageView>

        <Label fx:id="updateFinishTitle" layoutX="75.0" layoutY="85.0" text="The new version is finished " />
        <Label fx:id="updateNowTitle" layoutX="63.0" layoutY="126.0" text="Do you want to update it now ?" />
        <Label fx:id="updateNotNewVersionTitle" layoutX="95.0" layoutY="106.0" text="Is the latest version" />

        <Button fx:id="updateNow" layoutX="188.0" layoutY="153.0" mnemonicParsing="false" text="now" />
        <Button fx:id="updateLate" layoutX="76.0" layoutY="153.0" mnemonicParsing="false" text="late" />
        <Button fx:id="updateOk" layoutX="134.0" layoutY="153.0" mnemonicParsing="false" text="ok" />
      <Label layoutX="138.0" layoutY="59.0" text="v1.0" textFill="#0e0e0e" />
    </children>
</AnchorPane>
